<input type="text" class="bg-gray-500  text-gray-500 form-control " name="myvalue"  id="myvalue"   readonly />
<script>
$(document).ready(function(){
  $("#myInput").on("keyup", function() {

    var value = $(this).val().toLowerCase();
    $("#mem .main").filter(function() {
  
      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
      
      
    });
  });
  
  $(".main").click(function() {
  
  let ClickC = $(".nu").text();
   $('#myvalue').val(ClickC);
    copyToClipboard();
   
  $(".toast").toggleClass("w3-hide");
  setTimeout(function(){
   $(".toast").toggleClass("w3-animat-zoom , w3-hide");
   }, 1000);
  
  
  });
  
});
</script>

<script>
function copyToClipboard() {

let textBox = document.getElementById("myvalue");
textBox.select();
  navigator.vibrate([50]);
document.execCommand("copy");
}
</script>


